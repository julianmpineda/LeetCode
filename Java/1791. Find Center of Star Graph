/*
  Given that the graph is properly defined/created, you
  only need to check two points to see which one repeats.
  Could maybe be more readable with variables, but is so
  short seems unecessary.
*/

class Solution {
    public int findCenter(int[][] edges) {

        if (edges[0][0] == edges[1][0] || edges[0][0] == edges[1][1]) {
            return edges[0][0];
        } else {
            return edges[0][1];
        }
    }
}
